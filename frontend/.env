# Frontend Environment Variables
# Local Development: Uncomment localhost URLs
# Production: Use Railway URLs (commented by default for production)

# API Configuration - PRODUCTION (Railway)
VITE_API_URL=https://webkioskbe-production.up.railway.app/api
VITE_BACKEND_URL=https://webkioskbe-production.up.railway.app

# MoMo Payment API - PRODUCTION (Railway)
VITE_MOMO_API_URL=https://webkioskbe-production.up.railway.app/api/momo

# Local Development URLs (uncomment for local testing)
# VITE_API_URL=http://localhost:3001/api
# VITE_BACKEND_URL=http://localhost:3001
# VITE_MOMO_API_URL=http://localhost:3001/api/momo

# Robot Service Configuration
VITE_USE_REAL_ROBOTS=true
VITE_USE_MOCK_ROBOTS=false

# MQTT Configuration (DISABLED - not connecting directly)
# VITE_MQTT_BROKER_URL=
# VITE_MQTT_USERNAME=
# VITE_MQTT_PASSWORD=

# App Configuration
VITE_APP_NAME=AlphaAsimov Dashboard
VITE_APP_VERSION=1.0.0

# Development
VITE_DEV_TOOLS=true
VITE_ENV=development
VITE_DEBUG=true
